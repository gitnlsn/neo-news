{
  "name": "neo-news",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docker:up": "docker compose up",
    "docker:down": "docker-compose down --rmi all --volumes",
    "build": "next build",
    "typecheck": "npx biome format --write && biome check --write --unsafe && tsc --noEmit  ",
    "test": "dotenv -e .env.test -e .env -- vitest run",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:push:test": "dotenv -e .env.test -e .env -- prisma db push --force-reset",
    "db:all": "npm run db:push && npm run db:push:test && npm run db:generate",
    "db:studio": "dotenv -e .env -- prisma studio",
    "dev": "next dev --turbo",
    "postinstall": "prisma generate",
    "pipe": "npm run typecheck && npm run build && npm run test",
    "preview": "next build && next start",
    "start": "next start",
    "gen:use-case": "plop use-case"
  },
  "dependencies": {
